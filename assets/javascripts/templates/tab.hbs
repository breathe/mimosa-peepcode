<div class="tab">
    <h2>Tab for table <span>{{model.id}}</span></h2>

    <ul>
        {{#each tabItem in model.tabItems}}
            <li>
                <h3>{{tabItem.food.name}}<span>${{money tabItem.cents}}</span></h3>
            </li>
        {{else}}
            <li>
                <h3>Click a food to add it</h3>
            </li>
        {{/each}}
        <li class="total">
            <h3>Total <span>${{money model.cents}}</span></h3>
        </li>
    </ul>
</div>